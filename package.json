{
  "name": "getting",
  "version": "0.0.1",
  "description": "Type-safe get for an optional property at a nested level",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "npm test && npm run build",
    "test": "jest",
    "build": "tsc"
  },
  "keywords": [
    "typescript",
    "get",
    "elvis"
  ],
  "author": "Dmitriy Kubyshkin <dmitriy@kubyshkin.name>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "ts-jest": "^23.10.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^2.0.0",
    "typescript": "^3.1.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "src/.*\\.(ts|tsx|js)$"
  },
  "files": [
    "dist/*"
  ]
}
